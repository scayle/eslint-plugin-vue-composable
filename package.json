  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@types/eslint": "9.6.1",
    "dprint": "0.48.0",
    "eslint": "9.19.0",
    "eslint-doc-generator": "2.0.2",
    "eslint-plugin-eslint-plugin": "6.4.0",
    "eslint-plugin-n": "17.15.1",
    "globals": "15.14.0",
    "mocha": "11.1.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "volta": {
    "node": "22.13.0"
  }
