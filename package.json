{
  "name": "@scayle/eslint-plugin-vue-composable",
  "version": "0.2.1",
  "description": "Rules to avoid unpleasant composable problems",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "CHANGELOG.md",
    "lib",
    "docs",
    "LICENSE",
    "THIRD_PARTY_LICENSE"
